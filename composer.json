{
	"name": "martinpham/mongodb",
	"description": "Laravel MongoDB with PHP 7's MongoDB driver. Original from jenssegers/mongodb, Run `composer require \"mongodb/mongodb=^1.0.0@beta\"`",
	"keywords": ["laravel", "eloquent", "mongodb", "mongo", "database", "model"],
	"homepage": "https://gitlab.com/martinpham/laravel-mongodb",
	"authors": [{
		"name": "Martin Pham",
		"homepage": "http://martinpham.com"
	}],
	"license": "MIT",
	"require": {
		"illuminate/support": "^5.1",
		"illuminate/container": "^5.1",
		"illuminate/database": "^5.1",
		"illuminate/events": "^5.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.0|^5.0",
		"orchestra/testbench": "^3.1",
		"mockery/mockery": "^0.9",
		"satooshi/php-coveralls": "^0.6"
	},
	"autoload": {
		"psr-0": {
			"Martinpham\\Mongodb": "src/",
			"Martinpham\\Eloquent": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php",
			"tests/models",
			"tests/seeds"
		]
	}
}